{
  "name": "file-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'bin/**/*.ts' --exec ts-node app.js"
  },
  "lint-staged": {
    "{src,test}/**/*.ts*": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "googleapis": "^39.2.0",
    "less-middleware": "~2.2.1",
    "morgan": "~1.9.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.8",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2"
  }
}
